;; Test: Library module for multi-module test

(module multi-lib
  (export Point make-point point-add)

  (type Point (record (x Int) (y Int)))

  (fn make-point ((x Int) (y Int))
    (@intent "Create a point")
    (@spec ((Int Int) -> Point))
    (Point x y))

  (fn point-add ((a Point) (b Point))
    (@intent "Add two points")
    (@spec ((Point Point) -> Point))
    (Point (+ a.x b.x) (+ a.y b.y)))
)
